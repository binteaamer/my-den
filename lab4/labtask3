#include <stdio.h>
int main()
{
char op;
int num1, num2 , result ;

 printf ("enter an operator  ( +,-,*,/ )");
 scanf("%c" , &op  );
 printf ("enter num 1 and num 2");
 scanf ("%d" "%d", &num1 ,&num2 );

 if (op == '+') {
 result= num1 =num2 ;
 printf ("sum is : %d" , result);}
 else if (op== '-'){ 
 result= num1 -num2 ;
 printf (" difference is : %d", result);}
  else if (op== '*'){ 
 result= num1 *num2 ;
 printf (" product is : %d", result);}
  else if (op== '/'){ 
 result= num1 /num2 ;
 printf (" quotient is : %d", result);}
 else {printf("wrong operator ");}
 return 0;
 }
  
 
 


